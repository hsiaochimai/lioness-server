{
  "name": "lioness-app",
  "version": "1.0.0",
  "description": "This is a boilerplate project used for starting new projects!",
  "main": "index.js",
  "scripts": {
    "build": "npm run migrate-production 0 && npm run migrate-production && npm run seed-data",
    "test": "mocha --require test/setup.js --timeout 10000 --exit -R dot",
    "dev": "nodemon src/server.js",
    "migrate-production": "postgrator --config postgrator-production-config.js",
    "migrate-test": "postgrator --config postgrator-test-config.js",
    "seed-data": "node seeds/seedData.js",
    "migrate": "postgrator --config postgrator-config.js",
    "start": "node src/server.js",
    "predeploy": "npm audit && npm run migrate-production",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "engines": {
    "node": "10.16.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "chai-datetime": "^1.8.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "dotenv": "^7.0.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "flatted": "^2.0.2",
    "helmet": "^4.6.0",
    "isomorphic-fetch": "^3.0.0",
    "json.date-extensions": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "pg-connection-string": "^2.5.0",
    "postgrator": "^3.11.1",
    "query-string": "^6.14.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "postgrator-cli": "^3.3.0",
    "supertest": "^4.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsiaochimai/lioness-server.git"
  },
  "bugs": {
    "url": "https://github.com/hsiaochimai/lioness-server/issues"
  },
  "homepage": "https://github.com/hsiaochimai/lioness-server#readme"
}
